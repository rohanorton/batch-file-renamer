  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "mocha": "^2.3.3",
    "mock-fs": "^3.7.0"
  },
  "dependencies": {
    "es6-promisify": "^3.0.0",
